# .env.example — шаблон файла конфигурации среды
# Скопируйте как `.env` и заполните актуальными значениями перед запуском приложения
# Никогда не коммитьте реальные пароли или ключи доступа


# Основные настройки приложения
APP_NAME="Книжный магазин"

# Настройки подключения к базе данных
DB_DRIVER="dbdriver"  # (например, org.h2.Driver, org.postgresql.Driver)
DB_URL="jdbc:<dbtype>://<host>:<port>/<database>"
DB_USER="your-db-user"
DB_PASSWORD="your-db-password"

# Пути к ресурсам
VIEW_PATH="/absolute/path/to/your/views"


